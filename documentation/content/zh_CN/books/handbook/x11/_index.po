#, no-wrap
msgid ""
"\t <match target=\"font\">\n"
"\t     <test qual=\"all\" name=\"rgba\">\n"
"\t\t <const>unknown</const>\n"
"\t     </test>\n"
"\t     <edit name=\"rgba\" mode=\"assign\">\n"
"\t\t <const>rgb</const>\n"
"\t     </edit>\n"
"\t </match>\n"
msgstr ""

