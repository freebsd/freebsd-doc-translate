msgstr ""

#. type: Plain text
#: documentation/content/en/books/handbook/mirrors/_index.adoc:869
#: documentation/content/en/books/handbook/mirrors/_index.adoc:877
#: documentation/content/en/books/handbook/mirrors/_index.adoc:884
#: documentation/content/en/books/handbook/mirrors/_index.adoc:892
#: documentation/content/en/books/handbook/mirrors/_index.adoc:903
#: documentation/content/en/books/handbook/mirrors/_index.adoc:910
#: documentation/content/en/books/handbook/mirrors/_index.adoc:919
#: documentation/content/en/books/handbook/mirrors/_index.adoc:926
msgid "Available collections:"
msgstr ""

#. type: Plain text
#: documentation/content/en/books/handbook/mirrors/_index.adoc:871
msgid "ftp: A partial mirror of the FreeBSD FTP server."
msgstr ""

#. type: Plain text
#: documentation/content/en/books/handbook/mirrors/_index.adoc:872
#: documentation/content/en/books/handbook/mirrors/_index.adoc:879
#: documentation/content/en/books/handbook/mirrors/_index.adoc:886
#: documentation/content/en/books/handbook/mirrors/_index.adoc:894
#: documentation/content/en/books/handbook/mirrors/_index.adoc:905
#: documentation/content/en/books/handbook/mirrors/_index.adoc:927
msgid "FreeBSD: A full mirror of the FreeBSD FTP server."
msgstr ""

#. type: Labeled list
#: documentation/content/en/books/handbook/mirrors/_index.adoc:873
#, no-wrap
msgid "Netherlands"
