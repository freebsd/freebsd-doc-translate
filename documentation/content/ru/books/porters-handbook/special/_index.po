"PORTNAME=\ttokei\n"
"DISTVERSIONPREFIX=\tv\n"
"DISTVERSION=\t7.0.2\n"
"CATEGORIES=\tdevel\n"
msgstr ""
"PORTNAME=\ttokei\n"
"DISTVERSIONPREFIX=\tv\n"
"DISTVERSION=\t7.0.2\n"
"CATEGORIES=\tdevel\n"

